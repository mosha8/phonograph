{
  "name": "phonograph",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema src/server/db/prisma/schema.prisma --sql",
    "prisma:migrate-status": "prisma migrate status --schema src/server/db/prisma/schema.prisma",
    "prisma:migrate-create-only": "prisma migrate dev --schema=src/server/db/prisma/schema.prisma --create-only --skip-generate",
    "prisma:migrate-dev": "prisma migrate dev --schema=src/server/db/prisma/schema.prisma",
    "prisma:migrate-deploy": "prisma migrate deploy --schema=src/server/db/prisma/schema.prisma",
    "prisma:migrate-reset": "prisma migrate reset --schema=src/db/server/prisma/schema.prisma",
    "prisma:db-pull": "prisma migrate status --schema src/server/db/prisma/schema.prisma",
    "prisma:migrate-rollback": "prisma migrate resolve --schema src/server/db/prisma/schema.prisma  --rolled-back",
    "prisma:seed": "tsx src/server/db/seed.ts",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "graphql:codegen": "graphql-codegen --config src/server/graphql/codegen.ts",
    "jest": "jest",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
