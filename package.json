    "prisma:migrate-reset": "prisma migrate reset --schema=src/db/prisma/schema.prisma",
    "prisma:db-pull": "prisma migrate status --schema src/db/prisma/schema.prisma",
    "prisma:seed": "tsx src/db/seed.ts",
